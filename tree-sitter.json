{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "arktype",
      "camelcase": "ArkType",
      "title": "ArkType",
      "scope": "source.arktype",
      "file-types": [
        "typescript"
      ],
      "injection-regex": "^arktype$",
      "class-name": "TreeSitterArktype"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Typescript validator DSL",
    "authors": [
      {
        "name": "Jeff Martin",
        "email": "jeffmartin@gmail.com",
        "url": "https://jeffrom.co/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-arktype"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}